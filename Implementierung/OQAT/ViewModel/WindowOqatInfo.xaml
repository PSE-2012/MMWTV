<Window x:Class="Oqat.ViewModel.WindowOqatInfo"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="OQAT Info" Height="150" Width="300" Loaded="Window_Loaded">
    <StackPanel>
        <TextBlock TextWrapping="Wrap" Margin="3">
            Dieses Programm wurde im Rahmen der Veranstaltung "Praxis der Softwareentwicklung" 
            2012 am Karlsruher Institut für Technologie (KIT) entwickelt.
        </TextBlock>
        <WrapPanel>
            <Label Margin="3">Version:</Label>
            <Label Name="tbVersion" Margin="3"></Label>
        </WrapPanel>
    </StackPanel>
</Window>
